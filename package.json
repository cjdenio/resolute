		"ts-check": "tsc --noEmit",
		"format:check": "prettier --check .",
		"format:fix": "prettier --write .",
		"lint": "eslint app/javascript/"
	},
	"devDependencies": {
		"@types/rails__actioncable": "^6.1.6",
		"@typescript-eslint/eslint-plugin": "^5.6.0",
