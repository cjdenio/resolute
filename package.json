	"dependencies": {
		"@alpinejs/persist": "^3.7.1",
		"@fontsource/outfit": "^4.5.0",
		"@fortawesome/fontawesome-free": "^5.15.4",
		"@hotwired/stimulus": "^3.0.1",
		"@hotwired/turbo": "^7.0.1",
		"@hotwired/turbo-rails": "^7.1.0",
		"@rails/actioncable": "^6.0.0",
		"@rails/activestorage": "^6.0.0",
		"alpinejs": "^3.7.0",
		"autoprefixer": "^10.4.0",
		"cssnano": "^5.0.12",
		"esbuild": "^0.14.2",
		"postcss": "^8.4.4",
		"postcss-cli": "^9.0.2",
		"postcss-import": "^14.0.2",
		"postcss-nesting": "^10.0.2",
		"postcss-url": "^10.1.3",
		"tailwindcss": "^3.0.0",
		"typescript": "^4.4.4"
	},
	"scripts": {
		"build": "esbuild app/javascript/*.* --bundle --minify --sourcemap --inject:app/javascript/shim/jsx-shim.ts --outdir=app/assets/builds/",
